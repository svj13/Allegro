<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.StackedBarChart?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<SplitPane dividerPositions="0.3" xmlns="http://javafx.com/javafx/8"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.gui.UserPageController">
    <items>
        <JFXListView fx:id="listView" prefWidth="172.0"/>
        <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="500.0"
                    prefViewportHeight="436.0" prefViewportWidth="500.0">
            <content>
                <VBox fx:id="stats" alignment="CENTER" prefWidth="500.0" spacing="10.0"
                      BorderPane.alignment="CENTER">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0"
                               prefWidth="200.0" text="Latest Attempt">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <StackedBarChart fx:id="recentBar" alternativeRowFillVisible="false"
                                         categoryGap="0.0" horizontalGridLinesVisible="false"
                                         horizontalZeroLineVisible="false" layoutX="10.0"
                                         layoutY="247.0" legendVisible="false" maxHeight="30.0"
                                         maxWidth="500.0" verticalGridLinesVisible="false"
                                         verticalZeroLineVisible="false">
                            <xAxis>
                                <NumberAxis minorTickVisible="false" opacity="0.0"
                                            tickLabelsVisible="false" tickMarkVisible="false"/>
                            </xAxis>
                            <yAxis>
                                <CategoryAxis disable="true" opacity="0.0" tickLabelsVisible="false"
                                              tickMarkVisible="false" visible="false"/>
                            </yAxis>
                        </StackedBarChart>
                        <Label fx:id="chartTitle" alignment="CENTER" contentDisplay="CENTER"
                               text="Overall Stats" textAlignment="CENTER">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Label>
                        <StackedBarChart fx:id="stackedBar" alternativeRowFillVisible="false"
                                         categoryGap="0.0" horizontalGridLinesVisible="false"
                                         horizontalZeroLineVisible="false" legendVisible="false"
                                         maxHeight="30.0" maxWidth="500.0"
                                         verticalGridLinesVisible="false"
                                         verticalZeroLineVisible="false">
                            <xAxis>
                                <NumberAxis minorTickVisible="false" opacity="0.0"
                                            tickLabelsVisible="false" tickMarkVisible="false"/>
                            </xAxis>
                            <yAxis>
                                <CategoryAxis disable="true" opacity="0.0" tickLabelsVisible="false"
                                              tickMarkVisible="false" visible="false"/>
                            </yAxis>
                        </StackedBarChart>
                        <LineChart fx:id="lineChart" prefHeight="228.0" prefWidth="379.0"
                                   title="Past Results">
                            <xAxis>
                                <CategoryAxis side="BOTTOM" tickLabelGap="1.0"
                                              tickLabelsVisible="false" tickMarkVisible="false"/>
                            </xAxis>
                            <yAxis>
                                <NumberAxis autoRanging="false" minorTickVisible="false"
                                            prefHeight="300.0" side="LEFT" tickLabelGap="2.0"
                                            tickMarkVisible="false" tickUnit="20.0"/>
                            </yAxis>
                        </LineChart>
                        <Label prefHeight="15.0" prefWidth="56.0" text="Badges"
                               textAlignment="CENTER"/>
                        <AnchorPane maxHeight="85.0" maxWidth="351.0" minHeight="-Infinity"
                                    minWidth="-Infinity" prefHeight="85.0" prefWidth="351.0"/>
                    </children>
                    <padding>
                        <Insets top="20.0"/>
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
    </items>
</SplitPane>
